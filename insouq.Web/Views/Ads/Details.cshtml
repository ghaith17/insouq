@{
    string GetStringCondition(bool value)
    {
        if (value) return "Yes";
        return "No";
    }

    string GetTypeName()
    {
        if (Model.Ad.TypeId == StaticData.Motors_ID) return "Motors";
        if (Model.Ad.TypeId == StaticData.Properties_ID) return "Properties";
        if (Model.Ad.TypeId == StaticData.Jobs_ID) return "Jobs";
        if (Model.Ad.TypeId == StaticData.Business_ID) return "Business";
        if (Model.Ad.TypeId == StaticData.Classifieds_ID) return "Classifieds";
        if (Model.Ad.TypeId == StaticData.Electronics_ID) return "Electronics";
        if (Model.Ad.TypeId == StaticData.Numbers_ID) return "Numbers";
        if (Model.Ad.TypeId == StaticData.Services_ID) return "Services";

        return "";
    }
}


<!-- start modal -->
