<!-- End modal -->
<div class="notification-style" style="padding-top: 75px;">
    <div id="list-view" class="tab-pane fade active show containers trending-cat">
        <div class="row">
            <div class="col-md-12">
                <h1 class="section-title10"><img style="margin-right: 5px;" src="~/assets/img/32.png" alt="">@Model.Ad.Title</h1>
                <ol class="breadcrumb-style">
                    <li><a asp-controller="Home" asp-action="Index" class="breadcrumb-color">Home &gt; </a></li>
                    <li><a href="#" class="breadcrumb-color"> @GetTypeName()  &gt; </a></li>
                    <li><a href="#" class="breadcrumb-color"> @Model.Ad.CategoryEnName  &gt; </a></li>
                    <li class="breadcrumb-color"> Details</li>
                </ol>
            </div>
            <div class="col-md-8">
                @if (Model.Ad.TypeId != StaticData.Numbers_ID)
                {
                    <div class="container mt-5">
                        @{
                            var href = "#";

                            if (Model.Ad.CategoryId == StaticData.JobWanted_ID)
                            {
                                href = StaticData.SERVER_DOMAIN + Model.Ad.CV;

                            }
                        }
                        <div class="carousel-container position-relative row">

                                <div id="myCarousel" class="carousel slide" data-ride="carousel">
                                    <a href="@href">
                                        <div class="carousel-inner">
                                            @{
                                                var index = 0;
                                            }
                                            @for (index = 0; index < Model.Ad.Pictures.Count; index++)
                                            {
                                                if (index == 6) break;

                                                var src = StaticData.SERVER_DOMAIN + Model.Ad.Pictures[index].ImageURL;

                                                var active = "";

                                                if (index == 0)
                                                {
                                                    active = "active";
                                                }
                                                <div class="carousel-item @active" data-slide-number="@index">
                                                    <img style="height: 490px;" src="@src" class="d-block w-100" alt="..." data-type="image" data-toggle="lightbox" data-gallery="example-gallery">
                                                </div>

                                            }

                                        </div>
                                    </a>
                                </div>

                                <!-- Carousel Navigation -->
                                <div id="carousel-thumbs" class="carousel slide" data-ride="carousel">
                                    <div class="carousel-inner">
                                        <div class="carousel-item active">
                                            <div class="row mx-0">
                                                @{
                                                    var indexx = 0;
                                                }
                                                @foreach (var item in Model.Ad.Pictures)
                                                {
                                                    if (indexx == 6) break;

                                                    var selected = "";
                                                    if (indexx == 0)
                                                    {
                                                        selected = "selected";
                                                    }
                                                    var src = StaticData.SERVER_DOMAIN + item.ImageURL;
                                                    <div id="carousel-selector-@indexx" class="thumb col-4 col-sm-2 px-1 py-2 @selected" data-target="#myCarousel" data-slide-to="@indexx">
                                                        <img style="height: 90px;" src="@src" class="img-fluid" alt="...">
                                                    </div>

                                                    indexx++;
                                                }

                                            </div>
                                        </div>
                                        @if (Model.Ad.TypeId != StaticData.Numbers_ID)
                                        {
                                            if (index < Model.Ad.Pictures.Count)
                                            {
                                                <div class="carousel-item">
                                                    <div class="row mx-0">
                                                        @for (int i = index; i < Model.Ad.Pictures.Count; i++)
                                                        {
                                                            var src = StaticData.SERVER_DOMAIN + Model.Ad.Pictures[i].ImageURL;
                                                            <div id="carousel-selector-@i" class="thumb col-4 col-sm-2 px-1 py-2" data-target="#myCarousel" data-slide-to="6">
                                                                <img style="height: 90px;" src="@src" class="img-fluid" alt="...">
                                                            </div>
                                                        }
                                                    </div>
                                                </div>
                                            }
                                        }


                                    </div>
                                    <a class="carousel-control-prev" href="#carousel-thumbs" role="button" data-slide="prev">
                                        <span class="carousel-control-prev-icon" aria-hidden="true"> <i class="fa fa-caret-left" aria-hidden="true"></i></span>
                                        <span class="sr-only">Previous</span>
                                    </a>
                                    <a class="carousel-control-next" href="#carousel-thumbs" role="button" data-slide="next">
                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                        <span class="sr-only">Next</span>
                                    </a>
                                </div>
                        </div> <!-- /row -->
                    </div>
                }

                else if (Model.Ad.CategoryId == StaticData.PlateNumbers_ID)
                {
                    var numberPhoto = "";
                    if (!string.IsNullOrEmpty(Model.Ad.Photo2))
                    {
                        numberPhoto = StaticData.SERVER_DOMAIN + Model.Ad.Photo2;

                    }
                    else
                    {
                        numberPhoto = StaticData.SERVER_DOMAIN + Model.Ad.Photo;

                    }

                    var plateCodeClass = Model.Ad.En_Emirate.Replace(" ", "") + "-" + Model.Ad.En_PlateType.Substring(0, 1) + "Code-Details";
                    var plateNumberClass = Model.Ad.En_Emirate.Replace(" ", "") + "-" + Model.Ad.En_PlateType.Substring(0, 1) + "Number-Details";

                    <div class="container mt-5">
                        <div class="carousel-container position-relative row">
                            <div id="myCarousel" class="carousel slide" data-ride="carousel">
                                <div class="carousel-inner">
                                    <div class="carousel-item active" data-slide-number="0">
                                        <img src="@numberPhoto" class="d-block w-100">
                                        <h2 class="@plateCodeClass">@Model.Ad.PlateCode</h2>
                                        <h2 class="@plateNumberClass">@Model.Ad.Number</h2>
                                    </div>

                                </div>
                            </div>

                        </div>
                    </div>
                }
                else if (Model.Ad.CategoryId == StaticData.MobileNumbers_ID)
                {

                    var numberPhoto = StaticData.SERVER_DOMAIN + Model.Ad.Photo;

                    <div class="container mt-5">
                        <div class="carousel-container position-relative row">
