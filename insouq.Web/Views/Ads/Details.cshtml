                                <div class="carousel-inner">
                                    <div class="carousel-item active" data-slide-number="0">
                                        <img src="@numberPhoto" class="d-block w-100">
                                        <h2 class="mobile-Details">@Model.Ad.Code @Model.Ad.Number</h2>
                                    </div>

                                </div>
                            </div>

                        </div>
                    </div>
                }

                <div class="warp-title-style">
                    @if (User.Identity.IsAuthenticated)
                    {
                        if (Model.IsInFavorite)
                        {

                            <button type="button" class="change-btn14 float-style" name="button" style="cursor:pointer;" onclick="DetailsRemoveFromFavorite('@Model.Ad.Id')">
                                <i class="icon-padding fas fa-heart" aria-hidden="true"></i>Remove From Favorite
                            </button>

                        }
                        else
                        {
                            <button type="button" class="change-btn14 float-style" name="button" style="cursor:pointer;" onclick="DetailsAddToFavorite('@Model.Ad.Id')">
                                <i style="font-family: 'FontAwesome';" class="icon-padding fa fa-heart-o" aria-hidden="true"></i>Add To Favorite
                            </button>
                        }
                    }
                    else
                    {
                        <button type="button" class="change-btn14 float-style" name="button" style="cursor:pointer;" onclick="ShowLogin()">
                            <i style="font-family: 'FontAwesome';" class="icon-padding fa fa-heart-o" aria-hidden="true"></i>Add To Favorite
                        </button>
                    }


                    <button type="button" class="change-btn14 float-style" name="button" href="#" data-toggle="modal" data-target="#myModal156">
                        <i class="icon-padding fa fa-share-alt" aria-hidden="true"> </i>Share
                    </button>

                    <p><i style="font-family: 'FontAwesome';" class="fa fa-map-marker"></i>@Model.Ad.En_Location</p>
                </div>
                @{
                    var formatedValue = "";
                    var className = "span-post-price";
                }


                @if (Model.Ad.TypeId != StaticData.Jobs_ID && Model.Ad.TypeId != StaticData.Services_ID && Model.Ad.CategoryId != StaticData.ResidentialForRent_ID
