                    className = "span-post-without-price";

                }

                <span class="@className"> @Model.Ad.PostDate.ToString("dd MMM yyyy"), @Model.Ad.PostDate.ToString("HH:mm")</span>

                <h1 class="section-title15 payment-price1">@formatedValue</h1>

