@{
    ViewData["Title"] = "Motors DL";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["ActivePage"] = "Motors DL";
}

@using insouq.Shared.Utility

<div class="row">
    <div class="col-md-3">
        <a class="dl-color" asp-controller="SubCategory" asp-action="Index" asp-route-categoryId="@StaticData.Bike_ID">
            <div class="dl-style">
                <p>Bike Category</p>
            </div>
        </a>
    </div>
    <div class="col-md-3">
        <a class="dl-color" asp-controller="SubType" asp-action="Index" asp-route-categoryId="@StaticData.Bike_ID">
            <div class="dl-style">
                <p>Bike Sub Category</p>
            </div>
        </a>
    </div>
    <div class="col-md-3">
        <a class="dl-color" asp-action="EngineSizes">
            <div class="dl-style">
                <p>Engine Size</p>
            </div>
        </a>
    </div>
    <div class="col-md-3">
        <a class="dl-color" asp-action="Colors">
            <div class="dl-style">
                <p>Color</p>
            </div>
        </a>
    </div>
    <div class="col-md-3">
        <a class="dl-color" asp-action="FuelTypes">
            <div class="dl-style">
                <p>Fuel Type</p>
            </div>
        </a>
    </div>
    <div class="col-md-3">
        <a class="dl-color" asp-action="NoOfCylinders">
            <div class="dl-style">
                <p>Number Of Cylinders</p>
            </div>
        </a>
    </div>
    <div class="col-md-3">
        <a class="dl-color" asp-action="SellerTypes">
            <div class="dl-style">
                <p>Seller Type</p>
            </div>
        </a>
    </div>
    <div class="col-md-3">
        <a class="dl-color" asp-action="Wheels">
            <div class="dl-style">
                <p>Wheels</p>
            </div>
        </a>
    </div>
    <div class="col-md-3">
        <a class="dl-color" asp-action="FinalDriveSystems">
            <div class="dl-style">
                <p>Final Drive System</p>
            </div>
        </a>
    </div>
</div>

