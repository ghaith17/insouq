    <script>
        function FilterAgent() {
            var data = {
                AgentName: $("#AgentName").val(),
                MobileNumber: $("#MobileNumber").val(),
                AgentStatus: $('#AgentStatus').val(),
            }

            //console.log($("#MobileNumber").val() + " ,, " + MobileNumber);
            $.ajax({
                url: "/LeftSideBar/searchAgent",
                method: "POST",
                data: JSON.stringify(data),
                contentType: "application/json",
                success: function (response) {
                    $("#Agents").empty();
                    $("#Agents").html(response);
                    //                       console.log(response);
                },
                error: function (error) {
                    console.log("in 282 ", -1);
                }
            });

        }


        function EditAgent() {
            $.ajax({
                url: "/LeftSideBar/EditAgent",
                method: "GET",
                //data: JSON.stringify(data),
                contentType: "application/json",
                success: function (response) {
                    $("#ads").empty();
                    $("#ads").html(response);
                    //                       console.log(response);
                },
                error: function (error) {
                    console.log("in 510 ", -1);
                }

            });
        }

        function StaffFilter() {
            $.ajax({
                url: "/LeftSideBar/StaffFilter",
                method: "POST",
                //data: JSON.stringify(data),
                contentType: "application/json",
                success: function (response) {
