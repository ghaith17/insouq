        <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
        <script type="text/javascript">
            function DeleteAgent(id) {
                //console.log("in Delete : " + id);
                var Id = id;
                $.ajax({
                    url: `/AgencyProfile/RemoveAgent?agentId=${Id}`,
                    method: "POST",
                    success: function (response) {
                        $("#Agents").empty();
                        $("#Agents").html(response);
                        //                       console.log(response);
                    },
                    error: function (error) {
                        console.log("in 282 ", -1);
                    }
                });
            }

            function DisableAgent(id) {
                var Id = id;
                $.ajax({
                    url: "/LeftSideBar/DisableAgent",
                    method: "POST",
                    data: JSON.stringify(id),
                    contentType: "application/json",
                    success: function (response) {
                        $("#Agents").empty();
                        $("#Agents").html(response);
                    },
                    error: function (error) {
                        console.log("in 282 ", -1);
                    }
                });
            }


            function ActiveAgent(id) {
                var Id = id;
                $.ajax({
                    url: "/LeftSideBar/ActiveAgent",
                    method: "POST",
                    data: JSON.stringify(id),
                    contentType: "application/json",
                    success: function (response) {
                        $("#Agents").empty();
                        $("#Agents").html(response);
                    },
                    error: function (error) {
                        console.log("in 282 ", -1);
                    }
                });
            }

            $(function () {
                $('[id*=btnEdit]').on('click', function () {
                    var id = $(this).closest('tr').find('td').eq(0).html().trim();
//                    console.log(id);
                    let urlParams = "Id=" + id;
                  //  console.log("urlParams ", urlParams);
                    window.location.href = '@Url.Action("EditAgent","LeftSideBar")' + "?" + urlParams;

                });


                $('[id*=btnDelete]').on('click', function () {
                    var id = $(this).closest('tr').find('td').eq(0).html().trim();
                    //                    console.log(id);
                    // let urlParams = "Id= " + id;
                    // console.log("Id ", id);
                    DeleteAgent(id);
                });

                $('[id*=btnDisableAgent]').on('click', function () {
                    var id = $(this).closest('tr').find('td').eq(0).html().trim();
                    //                    console.log(id);
                    // let urlParams = "Id= " + id;
                    // console.log("Id ", id);
                    DisableAgent(id);
                });

                $('[id*=btnActiveAgent]').on('click', function () {
                    var id = $(this).closest('tr').find('td').eq(0).html().trim();
                    //                    console.log(id);
                    // let urlParams = "Id= " + id;
                    // console.log("Id ", id);
                    ActiveAgent(id);
                });



            });


        </script>

