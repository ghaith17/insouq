                    <section>
                        <div class="modal right fade " id="myModal2" tabindex="-1" role="dialog" aria-labelledby="myModalLabel2">
                            <div class="modal-dialog" role="document" id="myModal2">
                                <div class="modal-content">

                                    <div class="modal-header1 filter-modal">
                                        <h4 class="modal-title" id="myModalLabel2">FILTERS</h4>
                                        <button type="button" data-dismiss="modal" aria-label="Close" onclick="StaffFilter()">RESET</button>
                                        <button type="button" class="close cloase-style" data-dismiss="modal" aria-label="Close">
                                            <span aria-hidden="true">&times;</span>
                                        </button>
                                    </div>

                                    <div class="modal-body">
                                        <div class="padding-form par-dir3">

                                            <label>Agent name</label>
                                            <input type="text" class="popup-line-input" name="AgentName" id="AgentName" placeholder="">
                                        </div>
                                        <div class="padding-form par-dir3">

                                            <label>Mobile Number</label>

                                            <select class="popup-line-input" name="MobileNumber" id="MobileNumber">
                                                <option name="MobileNumber" value="All">All</option>
                                                @{
                                                    if (Model != null)
                                                    {
                                                        foreach (var item in @Model)
                                                        {
                                                            <option name="MobileNumber" value="@Html.DisplayFor(model => item.MobileNumber)">@Html.DisplayFor(model => item.MobileNumber)</option>
                                                        }
                                                    }
                                                }

                                            </select>

                                        </div>
                                        <div class="padding-form par-dir3">

                                            <label>Agent Status</label>

                                            <select class="popup-line-input" id="AgentStatus">
                                                <option name="Status" value="All">All</option>
                                                <option name="Status" value="Active">Active</option>
                                                <option name="Status" value="Disable">Disable</option>
                                                @*<option name="AgetnSatus" value="select">Select</option>*@
                                            </select>
                                        </div>

                                        <div class="padding-form par-dir3">
                                            @*      <button type="button" name="button" id="btnFilter" onclick="FilterAgent()">Filter results</button>*@
                                            <button type="button" class="" data-dismiss="modal" aria-label="Close" onclick="FilterAgent()">
                                                Filter results
                                                <span aria-hidden="true"></span>
                                            </button>
                                        </div>
                                    </div>


                                </div><!-- modal-content -->
                            </div><!-- modal-dialog -->
                        </div>
